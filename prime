
#include <stdio.h>

int main() {
    int i,j,n;
    int c=0,sum=0;
    scanf("%d",&n);
    for(i=2;i<=n;i++){
        for(j=2;j<=i;j++){
            if(i%j==0)
            break;
        }
         if(i==j)
      {
         printf("%d is a prime number\n",i);
         sum+=i;
         c++;}
        if(c==5)
        break;
    }
    printf("%d\n",sum);
   printf("%d",c);
  
    return 0;
}
